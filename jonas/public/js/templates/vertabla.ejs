<div class="page-header">
<h1>Tables</h1>
</div>
<div class="row">
    <div class="col-md-6">
      <table class="table">
        <thead>
          <tr>
            <th>Producto</th>
            <th>Precio</th>
          </tr>
        </thead>
        <tbody>
          <tr ng-repeat="empleado in empleados">
            <td>{{ empleado.name }}</td>
            <td>{{ empleado.age }}</td>
          </tr>
        </tbody>
      </table>
    </div>
</div>

