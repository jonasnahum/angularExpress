<div class="page-header">
<h1>Tables</h1>
</div>
<div class="row" div ng-app="sampleApp" ng-controller="customersCtrl">
    <div class="col-md-6">
      <table class="table">
        <thead>
          <tr>
            <th>Producto</th>
            <th>Precio</th>
          </tr>
        </thead>
        <tbody>
          <tr ng-repeat="articulo in articulos">
            <td>{{ articulo.producto }}</td>
            <td>{{ articulo.precio }}</td>
          </tr>
        </tbody>
      </table>
    </div>
</div>

